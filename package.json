{
  "name": "@ghbsom/vue-markdown",
  "author": "Shibin Ge <shbge123@gmail.com>",
  "bugs": "https://github.com/Gbin03/vue-markdown/issues",
  "dependencies": {
    "@types/hast": "^3.0.0",
    "@types/mdast": "^4.0.0",
    "devlop": "^1.0.0",
    "hast-util-to-jsx-runtime": "^2.0.0",
    "html-url-attributes": "^3.0.0",
    "mdast-util-to-hast": "^13.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "unified": "^11.0.0",
    "unist-util-visit": "^5.0.0",
    "vfile": "^6.0.0"
  },
  "type": "module",
  "version": "1.0.0",
  "description": "Vue component to render markdown",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "c8": "^10.0.0",
    "concat-stream": "^2.0.0",
